<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Pemilik.DaftarPeternak"
    android:orientation="vertical">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_marginTop="20dp"
        android:background="#DEF8EF">
        <View
            android:id="@+id/arrow_back"
            android:layout_width="30dp"
            android:layout_height="15dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/arrow_dropdown"
            android:rotation="90"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/image_daftar_peternak"/>
        <View
            android:id="@+id/image_daftar_peternak"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginBottom="10dp"
            android:layout_marginRight="30dp"
            android:background="@drawable/daftar_peternak1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/text_daftar_peternak"/>
        <TextView
            android:id="@+id/text_daftar_peternak"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Daftar Peternak"
            android:textSize="23dp"
            android:fontFamily="@font/montserrat"
            android:textColor="@color/black"
            android:layout_marginBottom="10dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
        <View
            android:layout_width="260dp"
            android:layout_height="8dp"
            android:layout_marginTop="35dp"
            android:layout_marginRight="50dp"
            android:background="@drawable/underline1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
        <View
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:background="@drawable/logo_tanpa_nama"
            android:layout_marginLeft="20dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/text_daftar_peternak"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginTop="20dp"
        android:layout_marginHorizontal="20dp"
        android:backgroundTint="@color/background1">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginVertical="20dp"
            android:layout_marginHorizontal="20dp"
            android:orientation="horizontal">
            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/foto_peternak_1"
                android:layout_width="110dp"
                android:layout_height="110dp"
                android:background="@drawable/foto_peternak"/>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:backgroundTint="@color/background"
                android:layout_marginLeft="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginVertical="10dp"
                    android:layout_marginHorizontal="10dp"
                    android:gravity="center_vertical"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/nama_peternak_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Nama : Valentino"
                        android:textSize="13dp"/>
                    <TextView
                        android:id="@+id/no_hp_peternak_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="No. hp : 000-000-000"
                        android:textSize="13dp"/>
                    <TextView
                        android:id="@+id/email_peternak_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Email : valentino123@gmail.com"
                        android:textSize="13dp"/>
                    <TextView
                        android:id="@+id/penempatan_peternak_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Penempatan : Kandang 1"
                        android:textSize="13dp"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginTop="20dp"
        android:layout_marginHorizontal="20dp"
        android:backgroundTint="@color/background1">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginVertical="20dp"
            android:layout_marginHorizontal="20dp"
            android:orientation="horizontal">
            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/foto_peternak_2"
                android:layout_width="110dp"
                android:layout_height="110dp"
                android:background="@drawable/foto_peternak"/>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:backgroundTint="@color/background"
                android:layout_marginLeft="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginVertical="10dp"
                    android:layout_marginHorizontal="10dp"
                    android:gravity="center_vertical"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/nama_peternak_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Nama : Valentino"
                        android:textSize="13dp"/>
                    <TextView
                        android:id="@+id/no_hp_peternak_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="No. hp : 000-000-000"
                        android:textSize="13dp"/>
                    <TextView
                        android:id="@+id/email_peternak_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Email : valentino123@gmail.com"
                        android:textSize="13dp"/>
                    <TextView
                        android:id="@+id/penempatan_peternak_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Penempatan : Kandang 1"
                        android:textSize="13dp"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginTop="20dp"
        android:layout_marginHorizontal="20dp"
        android:backgroundTint="@color/background1">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginVertical="20dp"
            android:layout_marginHorizontal="20dp"
            android:orientation="horizontal">
            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/foto_peternak_3"
                android:layout_width="110dp"
                android:layout_height="110dp"
                android:background="@drawable/foto_peternak"/>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:backgroundTint="@color/background"
                android:layout_marginLeft="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginVertical="10dp"
                    android:layout_marginHorizontal="10dp"
                    android:gravity="center_vertical"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/nama_peternak_3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Nama : Valentino"
                        android:textSize="13dp"/>
                    <TextView
                        android:id="@+id/no_hp_peternak_3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="No. hp : 000-000-000"
                        android:textSize="13dp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Email : valentino123@gmail.com"
                        android:textSize="13dp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Penempatan : Kandang 1"
                        android:textSize="13dp"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
    </androidx.cardview.widget.CardView>
</LinearLayout>